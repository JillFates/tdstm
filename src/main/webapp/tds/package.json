    "build-prod": "npm run compile-css & node ./node_modules/webpack/bin/webpack --profile --config ../../webapp/tds/webpack-prod.config.js --env=prod",
    "build-dev": "npm run compile-css & node ./node_modules/webpack/bin/webpack --profile --config ../../webapp/tds/webpack-dev.config.js",
