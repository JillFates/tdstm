<?xml version="1.0" encoding="UTF-8"?>
<process-definition xmlns="" name="Datacenter Move Operations - Standard Process" code="STD_PROCESS">
	<swimlane name="MANAGER">
		<assignment actor-id="Manager"/>
	</swimlane>
	<swimlane name="SUPERVISOR">
		<assignment actor-id="Supervisor"/>
	</swimlane>
	<swimlane name="MOVE_TECH">
		<assignment actor-id="Technician"/>
	</swimlane>
	<swimlane name="CLEANER">
		<assignment actor-id="Cleaner"/>
	</swimlane>
	<swimlane name="MOVER">
		<assignment actor-id="Mover"/>
	</swimlane>

	<transitions>
		<transition to="Hold" name="Hold" color="red"/>
		<transition to="Ready" name="Ready" color="yellow"/>
		<transition to="PoweredDown" name="Powered Down" color="orange"/>
		<transition to="Release" name="Release" color="green" />
		<transition to="Unracking" name="Unracking Started" color="blue"/>
		<transition to="Unracked" name="Unracked" color="blue"/>
		<transition to="Cleaned" name="Cleaned/Tagged"/>
		<transition to="OnCart" name="On Cart"/>
		<transition to="OnTruck" name="On Truck"/>
		<transition to="InTransit" name="In Transit"/>
		<transition to="EndTransit" name="End Transit"/>
		<transition to="OffTruck" name="Off Truck"/>
		<transition to="Staged" name="Staged"/>
		<transition to="Reracking" name="Rerack/Cabling Started"/>
		<transition to="Racked" name="Racked/Cabled"/>
		<transition to="QAVerified" name="QA Verification	"/>
		<transition to="Powered On" name="Powered On"/>
		<transition to="NetVerifying" name="Verify Network Started"/>
		<transition to="NetVerified" name="Network Verified"/>
		<transition to="SANVerifying" name="SAN Verify Started"/>
		<transition to="SANVerified" name="SAN Verified"/>
		<transition to="DBStarted" name="Database Tasks Started"/>
		<transition to="DBFinished" name="Database Tasks Finished"/>
		<transition to="AppStartup" name="Application Startup"/>
		<transition to="AppFinished" name="Application Finished"/>
		<transition to="AppVerifying" name="Application Verify Started"/>
		<transition to="AppVerified" name="Application Verified"/>
		<transition to="Completed" name="Completed"/>
		<transition to="Terminated" name="Terminated"/>
